#include "exinlibs\headfile.h"

void main()
{
    system_init(1);//初始化滴答计时器
    set_DCO_48MH();//注意这边的函数与其他例程不同，其他例程SMCLK为3MHZ，为了得到更高占空比可调范围，以及更高的电机基频率，本例程分频SMCLK为24MHZ
    IIC_init();
    while(1)
    {
        IIC_start();
        IIC_Send_Byte(0x33);
        while(Check_ACK()==0);
        IIC_stop();
    }
}
